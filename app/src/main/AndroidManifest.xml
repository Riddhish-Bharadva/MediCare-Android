<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rab.medicare">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".PaymentWebView"></activity>
        <activity android:name=".ShoppingCart" />
        <activity android:name=".OrderDetails" />
        <activity android:name=".MyOrders" />
        <activity android:name=".SearchProduct" />
        <activity android:name=".Profile" />
        <activity android:name=".ProductDetails" />
        <activity android:name=".SignIn" />
        <activity android:name=".SignUp" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>